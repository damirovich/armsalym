<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ARMzalogApp.Views.TotalNumberOfPhoto"
             xmlns:localF="clr-namespace:ARMzalogApp"
             Title="TotalNumberOfPhoto">
    <VerticalStackLayout>
        <Label 
             x:Name="totalNum"
             VerticalOptions="Center"  HorizontalOptions="Center" Margin="10,10,10,10"
             FontAttributes="Bold" FontSize="{Binding Source={x:Static localF:App.Settings}, Path=FontSize}"/>
    </VerticalStackLayout>
</ContentPage>